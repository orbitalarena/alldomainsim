{
  "metadata": {
    "name": "Mars Probe Mission",
    "description": "Civilian Mars transfer mission. Start in 200km LEO parking orbit at Cape Canaveral inclination. Plan and execute the Trans-Mars Injection (TMI) burn to escape Earth and reach Mars. Three DSN stations provide deep space tracking.",
    "version": "2.0",
    "author": "scenario_builder"
  },
  "environment": {
    "atmosphere": "us_standard_1976",
    "gravity": "inverse_square",
    "maxTimeWarp": 1024
  },
