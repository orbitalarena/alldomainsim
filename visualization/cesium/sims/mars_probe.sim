      "_custom": {
        "propulsion": {
          "modes": ["ROCKET"],
          "rocketEngine": "RL10"
        },
        "mass_kg": 5000,
        "notes": "Mars Explorer spacecraft in 200km LEO parking orbit. RL10 upper stage engine (500kN) for Trans-Mars Injection. A Hohmann-like transfer to Mars requires approximately 3.6 km/s of prograde delta-V from LEO."
      },
      "_quest": {
        "mode": "takeoff",
        "initialMsg": "MISSION CONTROL: Mars Explorer is in parking orbit. Plan the Trans-Mars Injection (TMI) burn.",
        "initialHint": "Press M for planner, N to create node. Add ~3.6 km/s prograde to reach Mars transfer orbit.",
        "milestones": [
          {
            "type": "alt",
            "value": 400000,
            "msg": "Climbing through 400km — departure trajectory confirmed.",
            "hint": "The TMI burn is raising your orbit. Continue burning prograde."
          },
          {
            "type": "alt",
            "value": 1000000,
            "msg": "Passing 1,000km — leaving LEO. DSN stations acquiring signal.",
            "hint": "You are well above the LEO belt. Your trajectory is becoming hyperbolic."
          },
          {
            "type": "alt",
            "value": 10000000,
            "msg": "10,000km altitude — well above the GEO belt. Mars-bound trajectory!",
            "hint": "You have departed the near-Earth environment. In a real mission, midcourse corrections would follow."
          }
        ]
      }
    },
